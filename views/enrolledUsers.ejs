<!DOCTYPE html>
<html lang="en" data-bs-theme="dark">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Enrolled Users</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
      crossorigin="anonymous"
    />
  </head>
  <body>
    <nav class="navbar navbar-expand-lg bg-body-tertiary">
      <div class="container-fluid">
        <div class="navbar-brand">YogiMaster</div>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
          <ul class="navbar-nav me-auto mb-2 mb-lg-0">
              <li class="nav-item">
                  <a class="nav-link active" aria-current="page" href="/trainer/upload-video">Upload</a>
              </li>
              <li class="nav-item">
                  <a class="nav-link active" aria-current="page" href="/trainer/trainer-videos">Videos</a>
              </li>
          </ul>
          
          <div class="d-flex">
              
            <div class="nav-item">
              <a class="nav-link active" href="/trainer/trainer-data"><svg xmlns="http://www.w3.org/2000/svg" width="35" height="35" fill="currentColor" class="bi bi-person-circle" viewBox="0 0 16 16">
                <path d="M11 6a3 3 0 1 1-6 0 3 3 0 0 1 6 0"/>
                <path fill-rule="evenodd" d="M0 8a8 8 0 1 1 16 0A8 8 0 0 1 0 8m8-7a7 7 0 0 0-5.468 11.37C3.242 11.226 4.805 10 8 10s4.757 1.225 5.468 2.37A7 7 0 0 0 8 1"/>
              </svg></a>
            </div>
          </div>
          
        </div>
      </div>
    </nav>

    <div class="d-flex justify-content-center">
      <h1 class="display-4">Users Enrolled</h1>
    </div>

    <div>
      <% enrolledUsers.forEach(user => { %>
      <div class="mb-2">
        <div class="row border border-3 m-2 p-2 rounded-3">
          <div
            class="col-xl-1 col-4 d-flex justify-content-center align-items-center"
          >
            <div>
              <img
                class=""
                src="<%= user.profileImage %>"
                style="width: 15vh; height: 15vh; border-radius: 15vh"
                alt="error"
              />
            </div>
          </div>
          <div
            class="col-xl-11 col-8 d-flex justify-content-start align-items-center"
          >
            <div class="w-100">
              <div class="fs-4">
                <%= user.fullName%>
              </div>
              <div class="fs-6 fw-light">
                <%= user.userName%>
              </div>
              <div class="row">
                <div class="col-xl-1 fs-6">
                    Age : <%= user.age%>
                  </div>
                  <div class="col-xl-6 fs-6">
                    Gender : <%= user.gender%>
                  </div>
              </div>
              <div class="fs-6">
                Level : <%= user.level%>
              </div>
            </div>
          </div>
        </div>
      </div>
      <% }); %>
    </div>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js"
      integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.min.js"
      integrity="sha384-0pUGZvbkm6XF6gxjEnlmuGrJXVbNuzT9qBBavbLwCsOGabYfZo0T0to5eqruptLy"
      crossorigin="anonymous"
    ></script>
  </body>
</html>
